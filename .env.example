# Pinata API Credentials - Get these from https://app.pinata.cloud/keys
PINATA_API_KEY=your_pinata_api_key
PINATA_API_SECRET=your_pinata_api_secret
PINATA_JWT=your_pinata_jwt_token

# Solana Network Configuration - Use 'mainnet-beta', 'devnet', or 'testnet'
NEXT_PUBLIC_SOLANA_NETWORK=devnet

# Gateway URL - Default Pinata gateway
NEXT_PUBLIC_PINATA_GATEWAY_URL=https://gateway.pinata.cloud/ipfs/
